pipeline {
    agent any
    // tools {
    //     python 'deafult'
    // }

    options {
        timeout(time: 5, unit: 'MINUTES')
    }

    stages {
        stage('Start app and BD') {
            steps{
                cmd_exec(cd API_integracion) 
                cmd_exec(docker-compose up -d --build)
            }
        }
        stage('Stop app and BD') {
            steps{
                cmd_exec(cd API_integracion) 
                cmd_exec(docker-compose down)
            }
        }
    }
}