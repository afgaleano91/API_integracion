pipeline {
    agent any
    // tools {
    //     python 'deafult'
    // }

    options {
        timeout(time: 5, unit: 'MINUTES')
    }

    stages {
        stage('Start app and BD') {
            steps{
                docker-compose up -d --build API_integracion/
            }
        }
        stage('Stop app and BD') {
            steps{
                docker-compose down API_integracion/
            }
        }
    }
}